\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
section{Experiment Design}
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Experiment Design
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Recruitment of Subjects
\end_layout

\begin_deeper
\begin_layout Itemize
Amazon's Mechanical Turk
\end_layout

\begin_layout Itemize
Screener survey
\end_layout

\end_deeper
\begin_layout Itemize
Task
\end_layout

\begin_deeper
\begin_layout Itemize
Button-Pressing task as in DellaVigna and Pope (2018)
\end_layout

\end_deeper
\begin_layout Itemize
Experiment Flow
\end_layout

\begin_deeper
\begin_layout Itemize
Assignment posted on M-Turk for a screener survey.
\end_layout

\begin_layout Itemize
Qualified subjects invited to initiate the experiment.
\end_layout

\begin_layout Itemize
Randomly assigned to the treatment group and to the role of employer or
 worker.
\end_layout

\begin_layout Itemize
Employer given 10 cents for every 100 points scored by the worker.
 Decides how to split these 10 cents before worker start working on the
 task.
\end_layout

\begin_layout Itemize
Worker observes the piece rate and then work on the task for upto 10 minutes.
\end_layout

\end_deeper
\end_deeper
