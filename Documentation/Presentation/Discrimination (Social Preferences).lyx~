section{Model}
\end_layout

\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Model
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\underbrace{max}_{e_{ij}\geq0}U_{ij}=\underbrace{max}_{e_{ij}\geq0}\left(F+(s_{i}+1_{Gift}\underbrace{\rho_{ij}}_{\rho_{i}+\Delta\rho_{ij}}+\underbrace{\alpha_{ij}}_{\alpha_{i}+\Delta\alpha_{ij}}v_{j}+p)e_{ij}-c(e_{ij})\right)\label{eq:utility}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\size small
Solving 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:utility"

\end_inset

 leads to following solution (when interior); 
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\[
e_{ij}^{\star}=c'^{-1}\left(s_{i}+1_{Gift}\rho_{ij}+\alpha_{ij}v_{j}+p\right)
\]

\end_inset


\end_layout

\begin_layout Itemize
Two forms of cost function
\end_layout

\begin_deeper
\begin_layout Itemize
Power Cost Function
\begin_inset Formula 
\[
c(e)=\frac{ke^{1+\gamma}}{1+\gamma}
\]

\end_inset


\end_layout

\begin_layout Itemize
Exponential Cost Function
\begin_inset Formula 
\[
c(e)=\frac{kexp^{\gamma e}}{\gamma}
\]

\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Treatments
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Piece Rate Treatments
\end_layout

\begin_deeper
\begin_layout Itemize

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $\rho_{ij}=0$
\end_inset

 and 
\begin_inset Formula $\alpha_{ij}=0$
\end_inset


\end_layout

\begin_layout Itemize
4 different piece rates 
\end_layout

\begin_layout Itemize
Helps identify 
\begin_inset Formula $s_{i},$
\end_inset

 
\begin_inset Formula $\gamma$
\end_inset

, and 
\begin_inset Formula $k$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Itemize
Altruism Treatments
\end_layout

\begin_deeper
\begin_layout Itemize
